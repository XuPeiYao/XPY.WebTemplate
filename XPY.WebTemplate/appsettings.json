{
  "Logging": {
    "LogFile": true,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "MiniProfiler": {
    "Enable": true,
    "RouteBasePath": "/profiler"
  },
  "DeviceDetector": true,
  "IP2Country": true,
  "Hangfire": {
    "Enable": true,
    "PathMatch": "/hangfire"
  },
  "Swagger": {
    "Enable": true,
    "Title": "服務名稱",
    "Description": "服務敘述"
  },
  "JWT": {
    "Issuer": "issuer",
    "Audience": "audience",
    "SecureKey": "secure key"
  },
  "Sentry": {
    "Enable": false,
    "DSN": ""
  },
  "RabbitMQ": {
    "Enable": false,
    "Hostname": "localhost",
    "VirtualHost": "/",
    "Port": 5672,
    "Username": "",
    "Password": ""
  }
}
