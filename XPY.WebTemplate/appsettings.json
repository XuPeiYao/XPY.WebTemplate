{
  "Logging": {
    "LogFile": true,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MiniProfiler": {
    "Enable": true,
    "RouteBasePath": "/profiler"
  },
  "DeviceDetector": true,
  "IP2Country": true,
  "Hangfire": {
    "Enable": true,
    "PathMatch": "/hangfire"
  },
  "Swagger": {
    "Enable": true,
    "Title": "服務名稱",
    "Description": "服務敘述"
  },
  "JWT": {
    "Issuer": "issuer",
    "Audience": "audience",
    "SecureKey": "secure key"
  }
}
